---
title: Home
layout: layouts/base.njk
eleventyNavigation:
  key: Home
  order: 1
scssCodeExample: |
  @use "uniform" as * with (
    $config: (
      important: true,
      utilities: (
        leading-trim: (
          shorthand: trim,
          properties: (leading-trim),
          variants: (
            both: both
          )
        )
      )
    )
  );
cssCodeExample: |
  .trim-both { 
    leading-trim: both !important; 
  }

  .align-left { 
    text-align: left !important; 
  }

  .align-center { 
    text-align: center !important; 
  }

  .align-right { 
    text-align: right !important; 
  }
