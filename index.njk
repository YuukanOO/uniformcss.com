---
title: Home
layout: layouts/base.njk
eleventyNavigation:
  key: Home
  order: 1
scssCodeExample: |
  @use "uniform" as * with (
    $config: (
      gradients: (
        hero: linear-gradient(to bottom, #101D36 0%, #023b7e 20%, #3988d7 50%, #e1f0ff 80%, #FFFFFF 100%)
      ),
      utilities: (
        background-color: (
          pseudos: (first, last, first-type, last-type, odd, even, empty)
        ),
        color: (
          responsive: true,
          pseuds: (group-focus)
        ),
      )
    )
  );
cssCodeExample: |
  .token.tag {
    color: var(--cyan-600);
  }

  .token.punctuation {
    color: var(--teal-300);
  }

  .token.attr-name {
    color: var(--pink-400);
  }

  .token.attr-value {
    color: var(--blue-400);
  }
